    <Style x:Key="DoubleColumnDataPointStyle" TargetType="{x:Type view:DoubleColumnDataPoint}">
        <Setter Property="Background" Value="Orange"/>
        <Setter Property="BorderBrush" Value="Black"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type view:DoubleColumnDataPoint}">
                    <Border  x:Name="Root"  Opacity="1">
                        <Border.ToolTip>
                            <ContentControl Content="{TemplateBinding FormattedDependentValue}"/>
                        </Border.ToolTip>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Column="0" VerticalAlignment="Bottom" Height="{TemplateBinding FirstHeight}" Background="Red">
                                <Rectangle Fill="#EEA863" StrokeThickness="0">
                                </Rectangle>
                            <Border BorderBrush="#CCFFFFFF" BorderThickness="0">
                                <Border BorderBrush="#77FFFFFF" BorderThickness="0"/>
                            </Border>
                            <Rectangle x:Name="SelectionHighlight1" Fill="Red" Opacity="0"/>
                            <Rectangle x:Name="MouseOverHighlight1" Fill="White" Opacity="0"/>
                            </Grid>
                            <Grid VerticalAlignment="Bottom" Grid.Column="1" Height="{TemplateBinding SecondHeight}" Background="Yellow">
                                <Rectangle Fill="#DC5E5F" StrokeThickness="0">
                                </Rectangle>
                                <Border BorderBrush="#CCFFFFFF" BorderThickness="0">
                                    <Border BorderBrush="#77FFFFFF" BorderThickness="0"/>
                                </Border>
                                <Rectangle x:Name="SelectionHighlight2" Fill="Red" Opacity="0"/>
                                <Rectangle x:Name="MouseOverHighlight2" Fill="White" Opacity="0"/>
                            </Grid>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <Style x:Key="LegendItemStyle_Gistogram" TargetType="{x:Type chartingToolkit:LegendItem}">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Margin" Value="5"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type chartingToolkit:LegendItem}">
                    <Border Height="26"   Background="Transparent">
                        <StackPanel Background="Transparent" Orientation="Horizontal" VerticalAlignment="Center">
                            <Rectangle Fill="{TemplateBinding Background}" Height="10" Margin="0,0,6,0" Stroke="{Binding Background}" StrokeThickness="1" Width="10"/>
                            <visualizationToolkit:Title Background="Transparent" FontFamily="Calibri" FontSize="15" Content="{TemplateBinding Content}"/>
                        </StackPanel>
                        <Border.ToolTip>
                            <ToolTip Content="{TemplateBinding Content}"/>
                        </Border.ToolTip>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="LegendItemStyle_HorisontalGistogram" TargetType="{x:Type chartingToolkit:LegendItem}">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Margin" Value="0 5 0 5"/>
        <Setter Property="MaxHeight" Value="30"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type chartingToolkit:LegendItem}">
                    <Border Height="26" MaxHeight="30"  Background="Transparent">
                        <visualizationToolkit:Title Background="Transparent" FontFamily="Calibri" FontSize="15" Content="{TemplateBinding Content}"/>
                        <Border.ToolTip>
                            <ToolTip Content="{TemplateBinding Content}"/>
                        </Border.ToolTip>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="HorizontalGistDataPointStyle" TargetType="{x:Type view:HorizontalColumnDataPoint}">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Margin" Value="0"/>
        <Setter Property="OverridesDefaultStyle" Value="True"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Width" Value="{Binding Width}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type view:HorizontalColumnDataPoint}">
                    <Border Width="{TemplateBinding Width}" HorizontalAlignment="Stretch" Height="{TemplateBinding Height}" x:Name="Root"  Opacity="1">
                        <Border.ToolTip>
                            <ContentControl Content="{TemplateBinding FormattedDependentValue}"/>
                        </Border.ToolTip>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid MaxHeight="30" Grid.Column="0" HorizontalAlignment="Left" >
                                <Rectangle HorizontalAlignment="Left" Width="{TemplateBinding ColorWidth}" Stroke="{TemplateBinding BorderBrush}" Fill="{TemplateBinding Background}">
                                </Rectangle>
                                <Rectangle x:Name="SelectionHighlight" Fill="Red" Opacity="0"/>
                                <Rectangle x:Name="MouseOverHighlight" Fill="White" Opacity="0"/>
                            </Grid>
                            <Border MaxHeight="30" x:Name="text" Background="#F1EFF2" BorderThickness="0 1 1 1" BorderBrush="#DEDCE1" HorizontalAlignment="Stretch"  Grid.Column="1">
                                <TextBlock HorizontalAlignment="Right" Foreground="#847F86" FontFamily="Calibri" TextAlignment="Right" Margin="4" FontSize="15" VerticalAlignment="Center" Text="{TemplateBinding FormattedDependentValue}"/>
                            </Border>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="ColorWidth" Value="0">
                            <Setter TargetName="text" Property="BorderThickness" Value="1"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="HorizontalColumnSeriesStyle" TargetType="{x:Type view:HorizontalColumnSeries}">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type view:HorizontalColumnSeries}">
                    <Canvas Background="White" x:Name="PlotArea"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ColumnDataPointStyle_Gistogram" TargetType="{x:Type chartingToolkit:ColumnDataPoint}">
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type chartingToolkit:ColumnDataPoint}">
                    <Border x:Name="Root" Opacity="1">
                        <Border.ToolTip>
                            <ContentControl Content="{TemplateBinding FormattedDependentValue}"/>
                        </Border.ToolTip>
                        <Grid Background="{TemplateBinding Background}">
                            <Rectangle Fill="{TemplateBinding Background}">
                            </Rectangle>
                            <Rectangle x:Name="SelectionHighlight" Fill="Red" Opacity="0"/>
                            <Rectangle x:Name="MouseOverHighlight" Fill="White" Opacity="0"/>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="MouseOverHighlight" Property="Opacity" Value="0.5"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


