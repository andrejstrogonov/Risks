                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <CheckBox Style="{StaticResource CheckBoxStyle}" Foreground="{Binding Path=Owner.Background, RelativeSource={RelativeSource Mode=TemplatedParent}}" Margin="0 0.05in 0 0" HorizontalAlignment="Left" Grid.Column="0" IsChecked="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Owner.Visibility, Mode=TwoWay, Converter={StaticResource FromBoolToVisibleConverter}}" />
                            <visualizationToolkit:Title Foreground="{Binding Path=Owner.Background, RelativeSource={RelativeSource Mode=TemplatedParent}}" Margin="0.05in 0 0 0" HorizontalAlignment="Left" Grid.Column="1" Content="{TemplateBinding Content}">
                                <visualizationToolkit:Title.Template>
                                    <ControlTemplate TargetType="visualizationToolkit:Title">
                                        <Border>
                                            <TextBlock Foreground="Black" TextWrapping="Wrap" Text="{TemplateBinding Content}"/>
                                        </Border>
                                    </ControlTemplate>
                                </visualizationToolkit:Title.Template>
                            </visualizationToolkit:Title>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


